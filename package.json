{
  "name": "pitchy",
  "description": "A simple pitch detection library.",
  "files": [
    "dist"
  ],
  "main": "./dist/pitchy.umd.cjs",
  "module": "./dist/pitchy.es.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/pitchy.es.mjs",
      "require": "./dist/pitchy.umd.cjs"
    }
  },
  "scripts": {
    "build": "run-s build:lib build:types",
    "build:lib": "vite build",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "fmt": "prettier --write .",
    "prepack": "npm run build",
    "test": "run-p test:*",
    "test:check": "tsc --noEmit",
    "test:fmt": "prettier --check .",
    "test:lint": "eslint --ext .js,.cjs,.mjs,.ts .",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianprime0509/pitchy.git"
  },
  "keywords": [
    "pitch",
    "detection",
    "music",
    "dsp"
  ],
  "author": {
    "name": "Ian Johnson",
    "email": "ian@ianjohnson.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianprime0509/pitchy/issues"
  },
  "homepage": "https://github.com/ianprime0509/pitchy#readme",
  "dependencies": {
    "fft.js": "^4.0.4"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.4.7",
    "jest-matcher-deep-close-to": "^3.0.2",
    "jest-matcher-percent-error": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.2",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  }
}
